<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>edu.uncc.weadvise.usermanagement.servlet.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TrainingController</servlet-name>
        <servlet-class>edu.uncc.weadvise.trainings.servlet.TrainingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiscussionController</servlet-name>
        <servlet-class>edu.uncc.weadvise.discussion.servlet.DiscussionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileController</servlet-name>
        <servlet-class>edu.uncc.weadvise.profileevaluation.servlet.ProfileController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/UserLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TrainingController</servlet-name>
        <url-pattern>/TrainingController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiscussionController</servlet-name>
        <url-pattern>/DiscussionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileController</servlet-name>
        <url-pattern>/ProfileController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
